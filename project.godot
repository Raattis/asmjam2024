; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="AsmJam2024"
run/main_scene="res://intro_scene.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameState="*res://game_state.tscn"
FadeToBlack="*res://fade_to_black.tscn"

[display]

window/size/viewport_width=600
window/size/viewport_height=400
window/size/initial_position_type=0
window/size/initial_position=Vector2i(1500, 600)
window/size/always_on_top=true

[editor_plugins]

enabled=PackedStringArray("res://addons/anthonyec.camera_preview/plugin.cfg")

[filesystem]

import/blender/enabled=false

[physics]

common/physics_ticks_per_second=120
common/max_physics_steps_per_frame=100
common/physics_interpolation=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
